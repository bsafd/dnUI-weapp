page{
 /*默认颜色配置*/
  --default: #dcdfe6;
	--primary:#FFDC1B;
  --primary-lt:#FFFBE9;
  --primary-dk:#F5C01F;
  --success:#15c377;
  --success-lt:#E4F8EF;
  --success-dk:#1DB574;
  --warning:#FFDC1B;
  --warning-lt:#FFFBE9;
  --warning-dk:#FFD956;
  --danger:#F8696A;
  --danger-lt:#FEEDED;
  --danger-dk:#F7494B;
  --info:#5556F7;
  --info-lt:#EAEAFD;
  --info-dk:#4C4DD9;

  /*字体颜色配置*/
  --text-basic: #303133;
  --text-common: #606266;
  --text-sub: #909399;
  --text-hold: #C0C4CC;
  --text-disabled: rgba(206, 206, 206, 0.6);

   /*边框颜色配置*/
  --border-basic: #DCDFE6;
  --border-common: #E4E7ED;
  --border-sub: #EBEEF5;
  --border-hold: #F2F6FC;
  
  /*全局圆角配置*/
  --round: 5px;

  /* Color 可以自定义相关配色 */
  --white: #ffffff;
  --light:#ededed;  
	--simple:#d2ddec;
  --dark:#3C4252;
	--black:#16202F;

  /* 标准色 */
  --breez:#08ddc1;
	--accent:#03a9f4;
	--red:#f96868;
	--pink:#e91e63;
	--purple:#9c27b0;
	--dpurple:#673ab7;
	--indigo:#3f51b5;
	--blue:#2196f3;
	--lblue:#03a9f4;
	--cyan:#00bcd4;
	--teal:#009688;
	--green:#4caf50;
	--lgreen:#8bc34a;
	--lime:#cddc39;
	--yellow:#ffeb3b;
	--amber:#ffc107;
	--orange:#0c0c0c;
	--dorange:#ff5722;
	--brown:#795548;
	--slate:#607d8b;
	--grey:#9e9e9e;

  /* 浅色 */
  --breez-lt:#E3FBF8;
	--accent-lt:#E1F5FD;
  --red-lt: #ffeee9;
  --pink-lt: #f9d7ea;
  --purple-lt: #ebd4ef;
  --dpurple-lt: #e1d7f0;
  --indigo-lt:#E8EAF5;
  --blue-lt: #cce6ff;
  --lblue-lt: #E1F5FD;
  --cyan-lt: #d2f1f0;
  --teal-lt:#E0F2F1;
  --green-lt: #d7f0db;
  --lgreen-lt: #e8f4d9;
  --lime-lt:#F9FBE8;
  --yellow-lt: #fef2ce;
  --amber-lt:#FFF8E2;
  --orange-lt: #fde6d2;
  --dorange-lt: #FBE9E7;
  --brown-lt: #ede1d9;
  --slate-lt:#ECEFF1;
  --grey-lt: #e7ebed;

  /* 阴影透明色 */
  --ShadowSize: 6rpx 6rpx 8rpx;
  --primary-sw:rgba(255,220,65,0.2);

  --breez-sw:rgba(33,219,192, 0.2);
  --accent-sw:rgba(25,167,240, 0.2);
  --red-sw: rgba(204, 69, 59, 0.2);
  --pink-sw: rgba(199, 50, 134, 0.2);
  --purple-sw: rgba(133, 33, 150, 0.2);  
  --dpurple-sw: rgba(88, 48, 156, 0.2);
  --indigo-sw:rgba(65,82,175, 0.2);
  --blue-sw: rgba(0, 102, 204, 0.2);  
  --lblue-sw: rgba(25,167,240, 0.2); 
  --cyan-sw: rgba(28, 187, 180, 0.2); 
  --teal-lt:rgba(20,148,135, 0.2); 
  --green-sw: rgba(48, 156, 63, 0.2);
  --lgreen-sw: rgba(139,193,87, 0.2);
  --lime-sw: rgba(204,219,80, 0.2);
  --yellow-sw: rgba(224, 170, 7, 0.2);
  --amber-lt:rgba(254,192,56, 0.2);
  --orange-sw: rgba(217, 109, 26, 0.2);
  --dorange-sw: rgba(254,89,50, 0.2);
  --brown-sw: rgba(140, 88, 53, 0.2);
  --slate-sw: rgba(97,124,137, 0.2);
  --grey-sw: rgba(114, 130, 138, 0.2);
  --blackShadow: rgba(26, 26, 26, 0.2);

  /* 渐变色 */
  --gradualRed: linear-gradient(45deg, #f43f3b, #ec008c);
  --gradualOrange: linear-gradient(45deg, #ff9700, #ed1c24);
  --gradualGreen: linear-gradient(45deg, #39b54a, #8dc63f);
  --gradualPurple: linear-gradient(45deg, #9000ff, #5e00ff);
  --gradualPink: linear-gradient(45deg, #ec008c, #6739b6);
  --gradualBlue: linear-gradient(45deg, #0081ff, #1cbbb4);

  /*全局背景颜色配置*/
  --bg-page: #fafafa;
  --bg-disabled: #e6e6e7;
  --hover:#fafafa;   /* hover 颜色*/
	--striped:rgba(120,120,120,0.05);
  background-color:#fafafa;
  /* font-family:PingFang-SC-Medium;eaeef1 */
}

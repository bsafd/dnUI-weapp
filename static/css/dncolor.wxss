.shadow-blur::before {
    content: "";
    display: block;
    background: inherit;
    filter: blur(10rpx);
    position: absolute;
    width: 100%;
    height: 100%;
    top: 10rpx;
    left: 10rpx;
    z-index: -1;
    opacity: 0.4;
    transform-origin: 0 0;
    border-radius: inherit;
    transform: scale(1, 1);
}
.opacity5,.dn-btn[disabled] {opacity: 0.5;}
.ukhover{background-color:var(--hover) !important;}
.shadowhover:active::after{background: rgba(0, 0, 0, 7%);}
.ukhoverdk:active::before {
  display: block;
  content: '';
  position: absolute;
  border-radius: inherit;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 7%);
}



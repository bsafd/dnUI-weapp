/* dn-card */
.dn-card{
  position:relative;
  display:-ms-flexbox;
  display:flex;-ms-flex-direction:column;
  flex-direction:column;
  min-width:0;
  word-wrap:break-word;
  background-clip:border-box;
  margin:0 30rpx 20rpx 30rpx;
  background: #fff;
}
.dn-card-full{margin-left:0rpx;margin-right:0rpx;}
.dn-card.dn-card-border{border:1rpx solid var(--border-sub);}
.dn-card-header {display:flex;justify-content:space-between;-webkit-box-align:center;align-items:center;padding:20rpx;margin-bottom:0;background-color:transparent;}
.dn-card-header-border {padding:20rpx;border-bottom:1rpx solid var(--border-sub);}
.dn-card-header-separator {padding-left:0rpx;padding-right:0rpx;margin-left:20rpx;margin-right:20rpx;}
.dn-card-title {padding:20rpx;font-size:32rpx;}
.dn-card-header .dn-card-title{padding:0rpx;}
.dn-card-body {-ms-flex:1 1 auto;flex:1 1 auto;padding:20rpx;}
.dn-card-content {-ms-flex:1 1 auto;flex:1 1 auto;padding:0rpx ;}
.dn-card-footer {padding:8rpx 20rpx;}
.dn-card-round {border-radius:10rpx}
.dn-card-round .dn-card-img-square image{vertical-align: top;border-radius:0 !important;}
.dn-card-round .dn-card-img-top image,
.dn-card-round .dn-card-img image {vertical-align: top;border-top-right-radius:10rpx !important;border-top-left-radius:10rpx !important;}
.dn-card-round .dn-card-img-bottom image,
.dn-card-round .dn-card-img image {vertical-align: top;border-bottom-right-radius:10rpx !important;border-bottom-left-radius:10rpx !important;}
.dn-card-img,
.dn-card-img-top,
.dn-card-img-bottom {border-radius:0rpx}
.dn-card-columns{-webkit-column-count:2;-moz-column-count:2;column-count:2;-webkit-column-gap:20rpx;-moz-column-gap:20rpx;column-gap:20rpx;orphans:1;widows:1;}
.dn-card-columns .card {display:inline-block;width:100%;}
.dn-card-media {display: flex;-webkit-box-align:center;align-items: flex-start;}
.dn-card-media {padding:20rpx 24rpx;-webkit-transition:background-color .2s linear;transition:background-color .2s linear}
.dn-card-media.dn-card-header-separator{padding-left:0rpx;padding-right:0rpx;}
.dn-card-media-body {-webkit-box-flex: 1;-ms-flex: 1;flex: 1;}
.dn-card-media-image {overflow: hidden;position:relative;display:inline-block;width:72rpx;height:72rpx;line-height:72rpx;text-align:center;border-radius:100%;background-color:#f5f6f7;text-transform:uppercase;margin-right:20rpx;}
.dn-card-subtitle{font-size:24rpx;color:var(--text-sub);font-weight:400;}
.dn-card-sidetitle{font-size:24rpx;align-self:center;color:var(--text-hold);font-weight:400;}

.card.popup{margin-bottom:0rpx !important;border-top-right-radius:20rpx !important;border-top-left-radius:20rpx !important;max-height:90vh}
.card.popup .popupHead{position: fixed;width:100%;z-index:999;border-top-right-radius:20rpx !important;border-top-left-radius:20rpx !important;}


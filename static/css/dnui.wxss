/* dn-btn */
.dn-btn {width: 100%;position: relative;border: 0 !important;border-radius: 8rpx;display: inline-block;overflow: inherit !important;white-space: nowrap;}
.dn-btn::after {
  content: "";
  position: absolute;
  width: 200%;
  height: 200%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scale(0.5, 0.5);
  transform: scale(0.5, 0.5);
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  left: 0;
  top: 0;
  border-radius: 16rpx;
}


.dn-btn.bg-white::after {border: 1px solid #eaeef1;}
.dn-btn-block {font-size:36rpx;height:90rpx;line-height:90rpx;}
.dn-btn-xs {width:auto;font-size:24rpx !important;height:40rpx !important;line-height:40rpx !important;border-radius: 4rpx !important;}
.dn-btn-sm {width:auto;font-size:30rpx !important;height:60rpx !important;line-height:60rpx !important;}
.dn-btn-class {border-radius:0 !important;height:100rpx !important;line-height:100rpx !important;}
.dn-btn-class-2 {border-radius:0 !important;height:100rpx !important;line-height:100rpx !important;font-size:30rpx !important;width:50% !important;}
.dn-btn-class-3 {display:block !important;font-size:28rpx !important;width:120%!important;}
.dn-round {border-radius: 45rpx !important;}
.dn-round.bg-white::after {border-radius: 90rpx !important;}
.dn-round-outline::after {border-radius: 90rpx !important;}
.dn-noradius,
.bg-white.dn-noradius::after,
.dn-noradius-outline::after {border-radius: 0 !important;}

/* dn-list */
.dn-media{position: relative;}
.dn-media {display: flex;align-items: flex-start;padding:26rpx 30rpx;-webkit-transition:background-color .2s linear;transition:background-color .2s linear}
.dn-media>view,
.dn-media>text {margin:0 20rpx}
.dn-media-center{-webkit-box-align:center;align-items:center}
.dn-media-title {-webkit-box-flex:1;flex:1 1 0%;overflow:hidden;}
.dn-media .avatar {flex-shrink:0}
.dn-media .align-center {-ms-grid-row-align:center;align-self:center}
.align-between{display:flex;align-self:stretch;flex-direction:column;}
.dn-media-body {flex: 1;min-width:0}
.dn-media-body .dn-media>view:first-child,
.dn-media-body .dn-media>text:first-child,
.dn-media:not(.dn-media-chat)>view:first-child,
.dn-media:not(.dn-media-chat)>text:first-child{margin-left:0}
.dn-media-body .dn-media>view:last-child,
.dn-media-body .dn-media>text:last-child,
.dn-media:not(.dn-media-chat)>view:last-child,
.dn-media:not(.dn-media-chat)>text:last-child{margin-right:0}
.dn-media-left {padding-right:0;margin-right:0 !important;}

/* 加50，根据avatar尺寸,父容器需要absolute定位，否则不出现边框 */
.dn-media-list-border .dn-media::after,
.dn-media-border::after{content: '';position: absolute;left: 30rpx;right: 0;bottom: 0;border-bottom: 3rpx solid var(--border-sub);-webkit-transform: scaleY(0.5);transform: scaleY(0.5);}
.dn-media-lineleft::after{left: 20rpx;}
.dn-media-lineright::after{right: 20rpx;}
.dn-media-border-no::after,
.dn-media-list-border .dn-media:last-child::after{border:0 !important;} 
/* .media-list-border .media{position: relative;} */
.dn-media-list-border.indent-order .dn-media::after{left:90rpx;}
.dn-media-list-border.indent .dn-media::after{left:122rpx;} 
.dn-media-list-border.indent-sm .dn-media::after{left:108rpx;}
.dn-media-list-border.indent-lg .dn-media::after{left:146rpx;}
.dn-media-list-border.indent-xl .dn-media::after{left:178rpx;}
.dn-media-border.indent::after{left:122rpx !important;} 
.dn-media-border.indent-sm::after{left:108rpx !important;}
.dn-media-border.indent-lg::after{left:146rpx !important;}
.dn-media-border.indent-xl::after{left:178rpx !important;}
.dn-media-border.indent-no::after{left:0rpx !important;}
.dn-media-list-gap {background:transparent;}
.dn-media-list-gap .dn-media {margin-bottom:20rpx !important;}
.dn-media-list-gap .dn-media {border-radius:8rpx;border: 1rpx solid var(--border-sub);margin-bottom: -1rpx;}
.dn-media-list-order {counter-reset:li;}
.dn-media-list-order .dn-media {position:relative;padding-left:90rpx;}
.dn-media-list-order .dn-media:before {position:absolute;left:0;top:50%;margin-top:-12px;content:counter(li);counter-increment:li;text-align:center;width:50px;display:inline-block;opacity:0.6;font-size:16px;font-weight:bold;}
.dn-media-list-striped .dn-media:not(.dn-media-list-header):not(.dn-media-list-footer):nth-child(even) {background-color:var(--striped) !important;}
.dn-media-list-arrow:before {
  content: " ";
  height: 11px;
  width: 11px;
  border-width: 2px 2px 0 0;
  border-color: rgba(192,196,204,1);
  border-style: solid;
  -webkit-transform: matrix(0.5, 0.5, -0.5, 0.5, 0, 0);
  transform: matrix(0.5, 0.5, -0.5, 0.5, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -5px;
  right: 30rpx;
}
.dn-media-list-header,
.dn-media-list-footer{background: #f2f5f7;padding: 8rpx 30rpx;display: flex;align-items: center;}
.dn-media-list-header.border {border-bottom:2rpx solid rgba(0,0,0,0.07);}
.dn-media-list-footer.border {border-top:2rpx solid rgba(0,0,0,0.07);}

.avatar {overflow: hidden;position:relative;display:inline-block;width:72rpx;height:72rpx;line-height:72rpx;text-align:center;border-radius:100%;background-color:#f5f6f7;text-transform:uppercase}
.avatar-i {flex-shrink:0;display:flex;align-items:center;justify-content:center;line-height:0;border-radius:100%;}
.avatar image {width:100%;height:100%;border-radius:100%;vertical-align:top}
.avatar-auto {width:auto !important;}
.avatar-dot {width:16rpx;height:16rpx;}
.avatar-xs {width:48rpx;height:48rpx;line-height:48rpx;font-size:16rpx}
.avatar-sm {width:58rpx;height:58rpx;line-height:58rpx;font-size:24rpx}
.avatar-lg {width:96rpx;height:96rpx;line-height:96rpx;font-size:40rpx}
.avatar-xl {width:128rpx;height:128rpx;line-height:128rpx;font-size:40rpx}
.avatar-xxl {width:220rpx;height:220rpx;line-height:220rpx;font-size:50rpx}
.avatar-more view {position:absolute;top:0;left:0;right:0;bottom:0;color:rgba(255,255,255,0.8);border-radius:100%;background-color:rgba(0,0,0,0.5);-webkit-transition:0.3s ease;transition:0.3s ease}
.avatar-square,
.avatar-square image,
.avatar-more.avatar-square view{border-radius:0 !important}
.avatar-border {border:8rpx solid rgba(250, 125, 125, 0.25);-webkit-background-clip:padding-box;background-clip:padding-box}
.avatar-list {-webkit-display:-webkit-inline-box;display:inline-flex}
.avatar-overlap>dn-avatar>view {border:2px solid #fff;box-shadow:0 0 15px rgba(0,0,0,0.2);-webkit-transition:.15s linear;transition:.15s linear}
.avatar-overlap view {margin-left:-16px}

.dn-triangle {border:16rpx solid;width:0;height:0;}
.dn-triangle-left {border-color:transparent var(--primary) transparent transparent;}
.dn-triangle-right {border-color:transparent transparent transparent var(--primary);}
.dn-triangle-top {border-color:transparent transparent var(--primary) transparent;}
.dn-triangle-bottom {border-color:var(--primary) transparent transparent transparent;}



/*checkbox  */

.dn-checkbox .wx-checkbox-input {width:36rpx;height:36rpx;border-radius:50%;}
/*checkbox选中后样式  */
.dn-checkbox .wx-checkbox-input.wx-checkbox-input-checked {background:var(--primary);width:38rpx !important;height:38rpx !important;border:none;}
/*checkbox选中后图标样式  */
.dn-checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {width:30rpx;height:30rpx;line-height:30rpx;text-align:center;font-size:20rpx;color:#fff;background:transparent;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}


.dn-upload {display:flex;display:-webkit-flex;flex-direction:row;flex-wrap:wrap;}
.dn-upload-item {width:153rpx;height:153rpx;border:1rpx solid var(--border-sub);box-sizing:border-box;border-radius:4rpx;position:relative;margin-bottom:36rpx;margin-right:26rpx;}
.dn-upload-item:nth-of-type(4n) {margin-right:0 !important;}
.dn-upload-img {width:153rpx;height:153rpx;border-radius:4rpx;}
.dn-upload-del {position:absolute;font-size:18px !important;right:-18rpx;top:-18rpx;color:var(--danger) !important;}
.dn-upload-add {color:var(--text-hold);display:flex;align-items:center;justify-content:center;}
.dn-upload-add text {font-size:84rpx;line-height:38rpx;height:48rpx;font-weight:100 !important;}

.dn-safearea-bottom {width: 100%;height: env(safe-area-inset-bottom);}

.dn-operation {width:100%;height:100rpx;box-sizing:border-box;overflow:hidden;background:rgba(255,255,255,0.9);position:relative;display:flex;align-items:center;justify-content:space-between;}
.dn-operation.fixbottom {box-sizing: content-box;position: fixed;left: 0;bottom: 0;padding-bottom: env(safe-area-inset-bottom);z-index: 999;}
.dn-operation::before {content:'';position:absolute;top:0px;right:0;left:0;border-top:1rpx solid #eaeef1;-webkit-transform:scaleY(0.5);transform:scaleY(0.5);}
.dn-operation-left {display:flex;align-items:center;}
.dn-operation-item {flex:1;display:flex;align-items:center;justify-content:center;flex-direction:column;position:relative;}
.dn-operation-right {height:100rpx;box-sizing:border-box;padding-top:0;}
.dn-operation-input {height:72rpx;border-radius:6rpx;padding-left:20rpx;padding-right:20rpx;flex:1;}
.dn-operation-inputbox {width:78%;justify-content:flex-start;} 


/*chatbox max-width:62%;*/
.dn-chatbox {box-sizing: content-box;max-width:446rpx;border-radius:10rpx;position:relative;padding:20rpx 22rpx;font-size:32rpx;color:#000;word-break:break-all;word-wrap:break-word;}
.dn-chatbox::before {content:"";position:absolute;width:0;height:0;top:20rpx;border:16rpx solid;}
.dn-chatbox-left {background: #fff;border: 1rpx solid #fff;display: inline-block;}
.dn-chatbox-left::before {right: 100%;border-color: transparent #fff transparent transparent;}
.dn-chatbox-right {background: #a0d5f3;border: 1rpx solid #a0d5f3;}
.dn-chatbox-right::before {left: 100%;border-color: transparent transparent transparent #a0d5f3;}
.dn-chatbox-success {background:#19be6b;border:1rpx solid #19be6b;color:#fff;}
.dn-chatbox-success::before {left:100%;border-color:transparent transparent transparent #19be6b;}
.dn-chatbox-info {background:#5c8dff;border:1rpx solid #5c8dff;color:#fff;}
.dn-chatbox-info::before {left:100%;border-color:transparent transparent transparent #5c8dff;}
.dn-chat-left,.dn-chat-right{display: flex;align-items: flex-start;padding-top:36rpx;}
.dn-chat-right{justify-content: flex-end;}
.dn-chat-left .avatar,
.dn-chat-right .avatar{width:80rpx;height:80rpx}
.dn-img-chatbox {position:relative;}
.dn-img-chatbox::after {content:'';position:absolute;height:200%;width:200%;border:1rpx solid #eaeef1;transform-origin:0 0;-webkit-transform-origin:0 0;-webkit-transform:scale(0.5);transform:scale(0.5);left:0;top:0;border-radius:20rpx;}
.dn-chat-img {max-width:200rpx;display:block;border-radius:10rpx;}
.dn-chat-voice {width:40rpx;height:40rpx;display:block;flex-shrink:0;}
.dn-chat-fail {width:50rpx;height:50rpx;display:block;flex-shrink:0;}


.dn-subsection {display:flex;align-items:center;justify-content:center;margin:20rpx 0;border-radius:10rpx;overflow:hidden;border:0;position:relative;}
.dn-subsection::after {content:"";position:absolute;width:200%;height:200%;border:4rpx solid var(--primary);-webkit-transform-origin:0 0;transform-origin:0 0;-webkit-transform:scale(0.5,0.5);transform:scale(0.5,0.5);-webkit-box-sizing:border-box;box-sizing:border-box;left:0;top:0;border-radius:20rpx;z-index:-1;}
.dn-subsection view {flex:1;font-size:28rpx;text-align:center;padding:10rpx 30rpx;color:var(--primary);position:relative;}
.dn-subsection view::after {content:"";position:absolute;height:100%;width:2rpx;border-right:2rpx solid var(--primary);-webkit-transform-origin:100% 0;transform-origin:100% 0;-webkit-transform:scaleX(0.5);transform:scaleX(0.5);right:0;top:0;}
.dn-subsection view:last-child::after {border-right:0;}



.dn-parallelogram {
  width: 100rpx;
  height: 50rpx;
  transform: skew(-10deg);
  background: var(--primary);
  margin-left: 10rpx;
}
.dn-crescent {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  box-shadow: 12rpx 12rpx 0 0 var(--warning);
}




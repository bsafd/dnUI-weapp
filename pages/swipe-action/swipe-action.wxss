@import '/static/css/dnui.wxss';

.container{
  background: #fff;
  padding-bottom: env(safe-area-inset-bottom);
}
.list-item {padding:30rpx 30rpx;display:flex;align-items:item;}
.list-item>view,.list-item>text {margin:0 20rpx}
.item-img {height:120rpx;width:120rpx;display:block}
.item-box {flex:1;width:70%;display:flex;flex-direction:column;justify-content:space-between}
.item-title {font-size:32rpx;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
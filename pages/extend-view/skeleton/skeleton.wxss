	.container{
		padding-bottom: 80rpx;
	}
	.dn-banner {
		width: 100%;
    height: 375rpx;
	}

	.dn-text {
		width: 100%;
		padding: 12rpx 30rpx 20rpx;
		box-sizing: border-box;
		color: #B3B3B3;
		font-size: 26rpx;
		text-align: right;
    margin-top: 8rpx
	}

	.dn-view {
		width: 100%;
		padding: 20rpx 30rpx;
		box-sizing: border-box;
	}


	.dn-title{
		padding:0 8rpx;
		box-sizing: border-box;
    display: inline-block;
	}
	.dn-link {
		width: 100%;
		padding: 30rpx;
		box-sizing: border-box;
		background: #fff;
		box-shadow: 0px 3rpx 20rpx rgba(183, 183, 183, 0.1);
		border-radius: 10rpx;
		color:#06c;
		margin-top: 20rpx;
    word-break: break-all;
	}
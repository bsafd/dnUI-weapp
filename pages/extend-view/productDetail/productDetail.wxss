@import '/static/css/dnui.wxss';
.container {
  padding-bottom: 110rpx;
}

.productHeader {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
}

.productTitle {
  width: 100%;
  font-size: 18px;
  line-height: 18px;
  font-weight: 500;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: center;box-sizing: content-box;
  
}

.productHeaderIcon {
  position: fixed;
  left: 10px;
  top: 0;
  display: flex;
  align-items: flex-start;
  height: 32px;
  transform: translateZ(0);
  z-index: 99999;
}

.dn-badge {
  background: #e41f19 !important;
  position: absolute;
  right: -14px;
}

.dn-icon {
  height: 32px !important;
  width: 32px !important;
  border-radius: 16px;
  display:flex;align-items:center;justify-content:center;line-height:0;
}


.productBanner {
  position: relative;
}

.productBannerTag {
  position: absolute;
  color: #fff;
  bottom: 30rpx;
  right: 0;
}

.productBannerImage {
  width: 100%;
  display: block;
}

/*顶部菜单*/

.dn-menu-box {
  box-sizing: border-box;
}

.dn-menu-header {
  font-size: 34rpx;
  color: #fff;
  height: 32px;
  display: flex;
  align-items: center;
}

.dn-top-dropdown {
  z-index: 9999 !important;
}

.dn-menu-itembox {
  color: #fff;
  padding: 40rpx 10rpx 0 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  font-size: 26rpx;
}

.dn-menu-item {
  width: 22%;
  height: 160rpx;
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  background: rgba(0, 0, 0, 0.4);
  margin-right: 4%;
  margin-bottom: 4%;
}

.dn-menu-item:nth-of-type(4n) {
  margin-right: 0;
}



.dn-menu-badge {
  position: absolute;
  top: -8px;
  right: -8px;
}

.dn-msg-badge {
  top: -10px;
}

.dn-icon-up {
  position: relative;
  display: inline-block;
  left: 50%;
  transform: translateX(-50%);
}

.dn-menu-text {
  padding-top: 12rpx;
}

.dn-opcity .dn-menu-text, .dn-opcity .dn-badge-box {
  opacity: 0.5;
  transition: opacity 0.2s ease-in-out;
}

/*顶部菜单*/

/*内容 部分*/


.dn-size {
  font-size: 24rpx;
  line-height: 24rpx;
}


.dn-borderRadius {
  border-bottom-left-radius: 24rpx;
  border-bottom-right-radius: 24rpx;
  overflow: hidden;
}

.dn-radius-all {
  border-radius: 24rpx;
  overflow: hidden;
}


.productDetail {
  box-sizing: border-box;
  color: #333;
}

.productDetailTitle {
  background: #fff;
  padding: 30rpx 0;
}

.dn-proPricebox {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #ff201f;
  font-size: 36rpx;
  font-weight: bold;
  line-height: 44rpx;
}

.dn-original-price {
  font-size: 26rpx;
  line-height: 26rpx;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
}



.dn-collection {
  color: #333;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  height: 44rpx;
}

.dn-scale {
  transform: scale(0.7);
  transform-origin: center center;
  line-height: 24rpx;
  font-weight: normal;
}

.dn-icon-collection {
  line-height: 20px !important;
  margin-bottom: 0 !important;
}

.dn-pro-titbox {
  font-size: 32rpx;
  font-weight: 500;
  position: relative;
  padding: 0 150rpx 0 30rpx;
  box-sizing: border-box;
}


.dn-share-btn {
  display: block;
  background: none;
  margin: 0;
  padding: 0;
  border-radius: 0;
}

.dn-tag-share {
  display: flex;
  align-items: center;
}

.dn-share-position {
  position: absolute;
  right: 0;
  top: 30rpx;
}

.dn-sale-info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 30rpx;
}
.dn-right {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}

.t-4 {
  top: 40rpx !important;
}
.dn-list-cell {
  position: relative;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  line-height: 26rpx;
  padding: 36rpx 30rpx;
  box-sizing: border-box;
}

.dn-list-cell::after {
  content: '';
  position: absolute;
  border-bottom: 1rpx solid #eaeef1;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  bottom: 0;
  right: 0;
  left: 116rpx;
}

.dn-last::after {
  border-bottom: 0 !important;
}

.dn-tag-coupon-box {
  display: flex;
  align-items: center;
}


.dn-cell-title {
  width: 86rpx;
  padding-right: 30rpx;
  flex-shrink: 0;
}

.dn-promotion-box {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding: 10rpx 0;
  width: 74%;
}

.dn-inline-block {
  display: inline-block !important;
  transform: scale(0.8);
  transform-origin: 0 center;
}


.w-76p {
  width: 76%;
}

.dn-addr-item {
  padding: 10rpx;
  line-height: 34rpx;
}

.dn-guarantee {
  background: #fdfdfd;
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx 30rpx 30rpx 30rpx;
  font-size: 24rpx;
}

.dn-guarantee-item {
  color: #999;
  padding-right: 30rpx;
  padding-top: 10rpx;
}

.pl-4px {
  padding-left: 4rpx;
}


.dn-cmt-btn {
  padding: 50rpx 0 30rpx 0;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dn-tag-cmt {
  min-width: 130rpx;
  padding: 20rpx 52rpx !important;
  font-size: 26rpx !important;
  display: inline-block;
}



.dn-product-img {
  transform: translateZ(0);
}

.dn-product-img image {
  width: 100%;
  display: block;
}

/*底部操作栏*/




.dn-operation {
  box-sizing: content-box;
  width: 100%;
  height: 100rpx;
  background: rgba(255, 255, 255, 0.98);
  position: fixed;
  display: flex;
  align-items: center;
  justify-content: space-between;
  z-index: 10;
  bottom: 0;
  left: 0;
  padding-bottom: env(safe-area-inset-bottom);
}

.dn-safearea-bottom {
  width: 100%;
  height: env(safe-area-inset-bottom);
}

.dn-operation::before {
  content: '';
  position: absolute;
  top: 0px;
  right: 0;
  left: 0;
  border-top: 1rpx solid #eaeef1;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.dn-operation-left {
  display: flex;
  align-items: center;
}

.dn-operation-item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: relative;
}

.opacity5 {
  opacity: 0.5;
}

.dn-scale-small {
  transform: scale(0.9);
  transform-origin: center center;
}

.dn-operation-right {
  height: 100rpx;
  box-sizing: content-box;
  padding-top: 0;
}

.dn-right-flex {
  display: flex;
  align-items: center;
  justify-content: center;
}

.dn-btn-equals {
  width: 90% !important;
  display: block !important;
  font-size: 28rpx !important;
}

.dn-badge-class {
  position: absolute;
  top: 0rpx !important;
  right:20rpx !important;
}



/*底部操作栏*/

/*底部选择弹层*/

.dn-popup {
  border-top-left-radius: 24rpx;
  border-top-right-radius: 24rpx;
  padding-bottom: env(safe-area-inset-bottom);
}

.dn-popup-box {
  position: relative;
  padding: 30rpx 0 100rpx 0;
}

.dn-popup-btn {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
}

.dn-icon-close {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}

.dn-product-box {
  display: flex;
  align-items: flex-end;
  font-size: 24rpx;
  padding-bottom: 30rpx;
}

.dn-popup-img {
  height: 200rpx;
  width: 200rpx;
  border-radius: 24rpx;
  display: block;
}

.dn-popup-price {
  padding-left: 20rpx;
  padding-bottom: 8rpx;
}

.dn-amount {
  color: var(--dorange);
  font-size: 36rpx;
}

.dn-number {
  font-size: 24rpx;
  line-height: 24rpx;
  padding-top: 12rpx;
  color: #999;
}

.dn-popup-scroll {
  height: 600rpx;
  font-size: 26rpx;
}

.dn-scrollview-box {
  padding: 0 30rpx 60rpx 30rpx;
  box-sizing: border-box;
}

.dn-attrTitle {
  padding: 10rpx 0;
  color: #333;
}

.dn-attrList {
  font-size: 0;
  padding: 20rpx 0;
}

.dn-attrItem {
  max-width: 100%;
  min-width: 200rpx;
  height: 64rpx;
  display: -webkit-inline-flex;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  background: #f7f7f7;
  padding: 0 26rpx;
  box-sizing: border-box;
  border-radius: 32rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
}

.dn-attrActive {
  background: var(--dorange-lt) !important;
  color: var(--dorange);
  font-weight: bold;
  position: relative;
}

.dn-attrActive::after {
  content: "";
  position: absolute;
  border: 1rpx solid var(--dorange);
  width: 100%;
  height: 100%;
  border-radius: 40rpx;
  left: 0;
  top: 0;
}

.dn-number-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0 30rpx 0;
  box-sizing: border-box;
}

/*底部选择弹层*/

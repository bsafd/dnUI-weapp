@import '/static/css/dnui.wxss';
page {
  height: 100%;
  overflow: hidden;
}

.scrollList {
  flex: 1;
}

.search-bar {
  display: flex;
  align-items: center;
  position: relative;
  padding: 30rpx 30rpx 30rpx;
  background-color: #fff;
  /* box-shadow: 0 2px 2px #ccc;  */
}

.search-bar-form {
  flex: 1;
  position: relative;
  border-radius: 32rpx;
  background: #f2f5f7;
}

.search-bar-box {
  display: flex;
  align-items: center;
  position: relative;
  padding-left: 20rpx;
  padding-right: 20rpx;
  height: 64rpx;
  z-index: 1;
}

.search-bar-input {
  line-height: normal;
  width: 100%;
  padding-left: 20rpx;
  font-size: 30rpx;
  color: #333;
}

.phcolor {
  font-size: 30rpx;
}

.icon-clear {
  height: 38rpx;
}

.icon-search {
  position: relative;
  height: 26rpx;
  margin-right: 20rpx;
  font-size: inherit;
}

.cancel-btn {
  padding-left: 30rpx;
}

.btn-primary {
  height: 62rpx;
  width: 122rpx;
  font-size: 30rpx;
  line-height: 62rpx;
  margin-left: 30rpx;
}

.search-result::before {
  display: none;
}

.search-result::after {
  display: none;
}







.dn-indexed-list-bar {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  z-index: 9999;
  position: absolute;
  top: 132rpx;
  right: 0;
  padding-right: 10rpx;
  width: 44rpx;
}

.dn-indexed-list-text {
  font-size: 22rpx;
}

.dn-indexed-list-bar.active {
  background-color: rgb(200, 200, 200);
}

.dn-indexed-list-alert {
  position: absolute;
  z-index: 20;
  width: 120rpx;
  height: 120rpx;
  right: 90rpx;
  top: 50%;
  margin-top: -60rpx;
  border-radius: 16rpx;
  text-align: center;
  line-height: 120rpx;
  font-size: 50rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.5);
}
